{
  "name": "qne-root",
  "version": "0.0.1",
  "private": true,
  "description": "Questionnaire Service â€“ Root",
  "devDependencies": {
    "concurrently": "^2.2.0",
    "nodemon": "^1.10.2",
    "npm-workspace": "^0.6.2"
  },
  "scripts": {
    "clean": "cd ./core && npm run clean && cd ../server && npm run clean && cd .. && npm-workspace clean",
    "start": "npm install && npm-workspace install && cd ./core && npm run build && cd ../server && npm run build",
    "TESTstart": "concurrently \"cd ./core && npm start\" \"cd ./server && npm start\" ",
    "TESTbuild-core": "cd core && npm install",
    "TESTbuild-server": "cd server && npm install",
    "TESTwatch-core-test": "nodemon -e js,json,handlebars node_modules/mocha/bin/mocha core/test/ --recursive --growl --reporter spec",
    "TESTwatch-client-test": "nodemon -e js,json,handlebars node_modules/mocha/bin/mocha core/test/ --recursive --growl --reporter spec",
    "TESTwatch-server-test": "nodemon -e js,json,handlebars node_modules/mocha/bin/mocha core/test/ --recursive --growl --reporter spec"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/filosofianakatemia/qne.git"
  },
