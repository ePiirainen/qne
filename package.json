{
  "name": "qne-root",
  "version": "0.0.1",
  "private": true,
  "description": "Questionnaire Service â€“ Root",
  "devDependencies": {
    "concurrently": "^2.2.0",
    "nodemon": "^1.10.2",
    "npm-workspace": "^0.6.2"
  },
  "scripts": {
    "clean": "cd ./core && npm run clean && cd ../server && npm run clean && cd .. && npm-workspace clean",
    "build:core-watch": "cd ./core && npm run build:watch",
    "test:core-watch": "cd ./core && npm run test:watch",
    "build:server-watch": "cd ./server && npm run build:watch",
    "test:server-watch": "cd ./server && npm run test:watch",
    "build:watch": "concurrently \"npm run build:core-watch\" \"npm run test:core-watch\" \"npm run build:server-watch\" \"npm run test:server-watch\" ",
    "start": "npm install && npm-workspace install && cd ./core && npm run postinstall-build && cd ../server && npm run postinstall-build",
    "TESTwatch-core-test": "nodemon -e js,json,handlebars node_modules/mocha/bin/mocha core/test/ --recursive --growl --reporter spec",
    "TESTwatch-client-test": "nodemon -e js,json,handlebars node_modules/mocha/bin/mocha core/test/ --recursive --growl --reporter spec",
    "TESTwatch-server-test": "nodemon -e js,json,handlebars node_modules/mocha/bin/mocha core/test/ --recursive --growl --reporter spec"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/filosofianakatemia/qne.git"
  },
  "contributors": [
    "Timo Tiuraniemi <timo.tiuraniemi@iki.fi>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/filosofianakatemia/qne/issues"
  },
  "homepage": "http://qne.io"
}
